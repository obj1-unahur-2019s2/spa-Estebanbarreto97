test "test bruno" {
	spa.atender(bruno)
	assert.that(bruno.tieneSed())
	assert.that(bruno.esFeliz())
}

test "test olivia" {
	spa.atender(olivia)
	assert.that(olivia.gradoDeConcentracion() > 0)
}